
top_srcdir= @top_srcdir@
srcdir	= @srcdir@
VPATH	= @srcdir@

MACHINE = @host@

# Programs used by "make":
#
CC	= @CC@
DEFS	= -D$(MACHINE)
CFLAGS	= -c @CFLAGS@ $(DEFS)
OFLAG	= @OFLAG@

SHELL	= /bin/sh
RM	= @RM@

# Source Directories

HDIR	= @HDIR@

# Files

HFILES	= @HFILES@
OBJS	= @TKOBJS@

all:	$(OBJS)

clean:
	$(RM) $(OBJS) core a.out

# Build rules

%.o : %.c
	$(CC) -c $(CFLAGS) $(OFLAG) $< -o $@

