;;
;; * Set compiler and compiler flags -- this includes pointing
;;   to the directory with our header files.
;;
(in-package "COMPILER")
(setq compiler::*cc* "@ECL_CC@")
(setq compiler::*cc-flags* "@CFLAGS@ -D@host@ @LSPCFLAGS@")
(setq compiler::*ld-flags* "-lgmp @LDFLAGS@ @CLIBS@")
#+dlopen
(setq compiler::*ld-shared-flags* "@SHARED_LDFLAGS@")
(load "sys:sysfun.lsp" :verbose nil :print nil)
